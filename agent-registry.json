{
  "agents": {
    "LocalSimulator": {
      "command": "node",
      "args": ["mcp-agent-simulator.js"],
      "type": "stdio",
      "autoStart": true,
      "description": "Local MCP-like simulator agent",
      "env": {}
    },
    "DockerMCP": {
      "command": "docker",
      "args": ["mcp", "gateway", "run"],
      "type": "stdio",
      "autoStart": true,
      "description": "Docker MCP Gateway - provides access to all Docker MCP servers (Obsidian, Puppeteer, etc.)",
      "env": {}
    }
  }
}
